// Program to do mathematical operations on two numbers a and b.
#include <stdio.h>

int main() {
    int a=10,b=5,sum,diff,mult,div,expo=1;
    sum=a+b;
    diff=a-b;
    mult=a*b;
    div=a/b;
    
    for(int i=0;i<b;i++){
        expo=expo*a;
    }
    
    printf("The sum of %d and %d is %d.\n",a,b,sum);
    printf("The difference of %d and %d is %d.\n",a,b,diff);
    printf("The product of %d and %d is %d.\n",a,b,mult);
    printf("The difference of %d and %d is %d.\n",a,b,div);
    printf("The power of %d raised to %d is %d.",a,b,expo);
    
    return 0;
}
